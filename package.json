    "graphql-shield": "^5.1.0",
    "graphql-yoga": "^1.17.0",
    "jsonwebtoken": "^8.4.0",
    "prisma-client-lib": "^1.24.0",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.2.1",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
